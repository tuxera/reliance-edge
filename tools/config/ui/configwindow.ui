                    <property name="text">
                     <string>Sarwate - midsized, fast</string>
                    </property>
                   </item>
                   <item>
                    <property name="text">
                     <string>Slice by 8 - largest, fastest</string>
                    </property>
                   </item>
                  </widget>
                 </item>
